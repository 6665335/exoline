    local function RIWJK_fake_script() -- Main.ShowPress 
    	local script = INST('LocalScript', Main)
    
    	local UIS = game:GetService("UserInputService")
    	
    	UIS.InputBegan:Connect(function(key)
    		if key.KeyCode == Enum.KeyCode.W then
    			script.Parent.W.Visible = true
    		else
    			if key.KeyCode == Enum.KeyCode.A then
    				script.Parent.A.Visible = true
    			else
    				if key.KeyCode == Enum.KeyCode.S then
    					script.Parent.S.Visible = true
    				else
    					if key.KeyCode == Enum.KeyCode.D then
    						script.Parent.D.Visible = true
    					end
    				end
    			end
    		end
    	end)
    		
    	UIS.InputEnded:Connect(function(key)
    		if key.KeyCode == Enum.KeyCode.W then
    			script.Parent.W.Visible = false
    		else
    			if key.KeyCode == Enum.KeyCode.A then
    				script.Parent.A.Visible = false
    			else
    				if key.KeyCode == Enum.KeyCode.S then
    					script.Parent.S.Visible = false
    				else
    					if key.KeyCode == Enum.KeyCode.D then
    						script.Parent.D.Visible = false
    					end
    				end
    			end
    		end
    	end)
    end
    coroutine.wrap(RIWJK_fake_script)()
    elseif val == false then
        GUI.Keystrokes:Destroy()
    end
end)

