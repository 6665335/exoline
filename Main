mainSector:AddDropdown("Places Teleport", {"Casual", "Competitive", "Deathmatch", "Trading", }, "-", function(val)
    	if val == "Casual" then
		game:GetService("TeleportService"):Teleport(301549746, client)
	elseif val == "Competitive" then
		game:GetService("TeleportService"):Teleport(1480424328, client)
	elseif val == "Deathmatch" then
		game:GetService("TeleportService"):Teleport(1869597719, client)
	elseif val == "Trading" then
        game:GetService("TeleportService"):Teleport(5325113759, client)
	end
end)

mainSector:AddDropdown("Play Sound", {"Planted", "Defused", "CT Win", "T Win", "TTT a", "TTT b", "Beep", "Explosion", "Rescued", "Becky", }, "-", function(val)
    	if val == "Planted" then
		workspace.Sounds.Arm:Play()
	elseif val == "Defused" then
		workspace.Sounds.Defuse:Play()
	elseif val == "CT Win" then
		workspace.Sounds.CT:Play()
	elseif val == "T Win" then
        workspace.Sounds.T:Play()
	elseif val == "TTT a" then
        workspace.RoundEnd:Play()
	elseif val == "TTT b" then
        workspace.RoundStart:Play()
	elseif val == "Beep" then
        workspace.Sounds.Beep:Play()
	elseif val == "Explosion" then
        workspace.Sounds.Explosion:Play()
    elseif val == "Rescued" then
        workspace.Sounds.Rescue:Play()
   elseif val == "Becky" then
        workspace.Sounds.Becky:Play()
	end
end)

local SL = mainSector:AddToggle("Spectator List", false, function(val)
if val == true then 
    local SpectatorsList = INST("ScreenGui")
    local Frame = INST("Frame")
    local Players = INST("TextLabel")
    local ListText = INST("TextLabel")
    
    SpectatorsList.Name = "SpectatorsList"
    SpectatorsList.Parent = GUI
    SpectatorsList.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    Frame.Parent = SpectatorsList
    Frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    Frame.BorderColor3 = Color3.fromRGB(255, 255, 255)
    Frame.BorderSizePixel = 0
    Frame.Position = UDim2.new(0, 2, 0, 380)
    Frame.Size = UDim2.new(0, 164, 0, 20)
    
    Players.Name = "Players"
    Players.Parent = Frame
    Players.Active = true
    Players.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    Players.BackgroundTransparency = 1.000
    Players.BorderColor3 = Color3.fromRGB(255, 255, 255)
    Players.BorderSizePixel = 0
    Players.Position = UDim2.new(0, 0, 1, 0)
    Players.Size = UDim2.new(0, 164, 0, 54)
    Players.ZIndex = 1000
    Players.Font = Enum.Font.RobotoMono
    Players.Text = "Players"
    Players.TextColor3 = Color3.fromRGB(255, 255, 255)
    Players.TextSize = 14.000
    Players.TextYAlignment = Enum.TextYAlignment.Top
    
    ListText.Name = "List Text"
    ListText.Parent = Frame
    ListText.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    ListText.BorderColor3 = Color3.fromRGB(255, 255, 255)
    ListText.Size = UDim2.new(0, 164, 0, 20)
    ListText.Font = Enum.Font.RobotoMono
    ListText.Text = "Spectators"
    ListText.TextColor3 = Color3.fromRGB(255, 255, 255)
    ListText.TextSize = 14.000
    
    local function CCZDN_fake_script() -- Frame.Drag 
    	local script = INST('LocalScript', Frame)
    
    	local UIS = game:GetService("UserInputService")
    	function dragify(Frame)
    		dragToggle = nil
    		local dragSpeed = 0.5
    		dragInput = nil
    		dragStart = nil
    		local dragPos = nil
    		function updateInput(input)
    			local Delta = input.Position - dragStart
    			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
    			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
    		end
    		Frame.InputBegan:Connect(function(input)
    			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
    				dragToggle = true
    				dragStart = input.Position
    				startPos = Frame.Position
    				input.Changed:Connect(function()
    					if input.UserInputState == Enum.UserInputState.End then
    						dragToggle = false
    					end
    				end)
    			end
    		end)
    		Frame.InputChanged:Connect(function(input)
    			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
    				dragInput = input
    			end
    		end)
    		game:GetService("UserInputService").InputChanged:Connect(function(input)
    			if input == dragInput and dragToggle then
    				updateInput(input)
    			end
    		end)
    	end
    	
    	dragify(script.Parent)
    end
    coroutine.wrap(CCZDN_fake_script)()
    local function NOPZPRT_fake_script() -- SpectatorsList.LocalScript 
    	local script = INST('LocalScript', SpectatorsList)
    
    	function GetSpectators()
    		local CurrentSpectators = ""
    		for i,v in pairs(game.Players:GetChildren()) do 
    			pcall(function()
    				if v ~= game.Players.LocalPlayer then
    					if not v.Character then 
    						if (v.CameraCF.Value.p - game.Workspace.CurrentCamera.CFrame.p).Magnitude < 10 then 
    							if CurrentSpectators == "" then
    								CurrentSpectators = v.Name
    							else
    								CurrentSpectators = CurrentSpectators.. "\n" ..v.Name
    							end
    						end
    					end
    				end
    			end)
    		end
    		return CurrentSpectators
    	end
    	spawn(function()
    		while wait(0.01) do
    			if SpectatorsList.Enabled then
    				Players.Text = GetSpectators()
    			end
    		end
    	end)
    end
coroutine.wrap(NOPZPRT_fake_script)()
elseif val == false then
        GUI.SpectatorsList:Destroy()
    end
end)

