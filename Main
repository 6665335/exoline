local PL = mainSector:AddToggle("Player Trail", false, function(val)
    if val == true then
        PlayerTrail = game:GetService("RunService").RenderStepped:Connect(function()
            pcall(function()
                local LFoot = INST("Attachment",client.Character.LeftFoot)
                local RFoot = INST("Attachment",client.Character.RightFoot)
                local Trail = INST("Trail")
                Trail.Attachment0 = LFoot
                Trail.Attachment1 = RFoot
                Trail.Lifetime = 3
                Trail.Parent = LFoot.Parent
                Trail.Color3 = Color3.new(255,255,255)
            end)
        end)
    elseif val == false then
        PlayerTrail:Disconnect()
    end
end)

