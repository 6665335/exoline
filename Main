    local function FPRMLZO_fake_script() -- ScreenLabel.timescript 
    	local script = INST('LocalScript', ScreenLabel)
    
    	local ContainerFrame = script.Parent:WaitForChild("Container")
    	local TimeLabel = ContainerFrame:WaitForChild("Time")
    	
    	while wait() do
    		local seconds = os.date("*t") ["sec"]
    		local minutes = os.date("*t") ["min"]
    		local hours = os.date("*t") ["hour"]
    	
    		if tonumber(seconds) <= 9 then
    			seconds = "0"..seconds
    		end
    	
    		if tonumber(minutes) <= 9 then
    			minutes = "0"..minutes
    		end
    	
    		if tonumber(hours) <= 9 then
    			hours = "0"..hours
    		end
    	
    		TimeLabel.Text = hours..":"..minutes..":"..seconds
    	end
    end
    coroutine.wrap(FPRMLZO_fake_script)()
	elseif val == false then
		GUI.watermark:Destroy()
	end
end) 

